<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/view_course_list_id">

    <fragment
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:id="@+id/course_list_frag"
        android:name="rmfarc.tacoma.uw.edu.fragmentslabthethird.CourseListFragment"
        tools:layout="@layout/fragment_course_list" />

    <fragment
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_height="match_parent"
        android:id="@+id/course_detail_frag"
        android:name="rmfarc.tacoma.uw.edu.fragmentslabthethird.CourseDetailFragment"
        tools:layout="@layout/fragment_course_item" />
</LinearLayout>
